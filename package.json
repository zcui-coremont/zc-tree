{
  "name": "zc-tree",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/zc-tree.umd.js",
  "module": "./dist/zc-tree.es.js",
  "exports": {
    ".": {
      "import": "./dist/zc-tree.es.js",
      "require": "./dist/zc-tree.umd.js"
    }
  },
  "homepage": "https://origami-z.github.io/zc-tree/",
  "scripts": {
    "start": "npm run storybook",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -- --preview-url=/zc-tree/iframe.html",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.5.0-alpha.26",
    "@storybook/addon-essentials": "^6.5.0-alpha.26",
    "@storybook/addon-links": "^6.5.0-alpha.26",
    "@storybook/react": "^6.5.0-alpha.26",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "storybook-builder-vite": "^0.1.14",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
